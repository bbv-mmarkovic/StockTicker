<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
        <SolidColorBrush x:Key="DefaultBackgroundColor" Color="White" />

        <Color x:Key="SelectionColor">#FFfca160</Color>

        <SolidColorBrush x:Key="{x:Static SystemColors.HighlightTextBrushKey}" Color="Black"/>

        <LinearGradientBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" StartPoint="0.5,0" EndPoint="0.5,1">
            <LinearGradientBrush.GradientStops>
                <GradientStopCollection>
                    <GradientStop Color="#FFc69661" Offset="0" />
                    <GradientStop Color="#FFe8aa67" Offset="0.041600000113248825" />
                    <GradientStop Color="#FFf8b26a" Offset="0.09300000220537186" />
                    <GradientStop Color="#FFfca160" Offset="0.39899998903274536" />
                    <GradientStop Color="#FFfb8c3c" Offset="0.4000000059604645" />
                    <GradientStop Color="#FFfc9c40" Offset="0.595300018787384" />
                    <GradientStop Color="#FFffd086" Offset="1" />
                </GradientStopCollection>
            </LinearGradientBrush.GradientStops>
        </LinearGradientBrush>
</ResourceDictionary>